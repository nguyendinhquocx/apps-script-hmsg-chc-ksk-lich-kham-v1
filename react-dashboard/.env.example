# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_project_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application Configuration
REACT_APP_APP_NAME=Hệ thống quản lý lịch khám
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Debug and Logging
REACT_APP_DEBUG_LOGS=false
REACT_APP_LOG_LEVEL=info

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_MAX_RECORDS_PER_PAGE=100
REACT_APP_DEFAULT_PAGE_SIZE=20

# Rate Limiting
REACT_APP_RATE_LIMIT_REQUESTS=30
REACT_APP_RATE_LIMIT_WINDOW=60000

# Security
REACT_APP_ENABLE_CSRF_PROTECTION=true
REACT_APP_SESSION_TIMEOUT=3600000

# Features
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_CHARTS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# UI Configuration
REACT_APP_THEME=light
REACT_APP_LANGUAGE=vi

# Performance
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_CACHE_DURATION=300000

# Analytics (Optional)
# REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id
# REACT_APP_ENABLE_ANALYTICS=false